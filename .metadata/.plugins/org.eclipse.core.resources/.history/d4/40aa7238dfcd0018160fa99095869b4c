package instance_enshu;

public class Robot {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		public String name = null;

	}

	public void talk(){
		System.out.println(this.name + "が話す");
	}
	public void walk(){
		System.out.println(this.name + "が歩く");
	}
	public void run(){
		System.out.println(this.name + "が走る");
	}
}
